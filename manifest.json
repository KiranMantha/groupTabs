{
  "manifest_version": 3,
  "name": "Group Tabs",
  "description": "Save tabs as a collection and view later.",
  "version": "0.0.0.6",
  "author": "Kiran Mantha",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Group Tabs",
    "default_popup": "collapse_tab.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'unsafe-eval'; object-src 'self'"   
  }
}